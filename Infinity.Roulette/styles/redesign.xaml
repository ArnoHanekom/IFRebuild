    <ControlTemplate x:Key="SearchResultsTabItem" TargetType="{x:Type TabItem}">
        <Border x:Name="PanelBorder" BorderThickness="2,2,2,0" Width="150" CornerRadius="5,5,0,0" Padding="10">
            <Grid x:Name="Panel">
                <TextBlock x:Name="HeaderText" VerticalAlignment="Center" HorizontalAlignment="Center"
                   Text="{TemplateBinding HeaderedContentControl.Header}"/>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="TabItem.IsSelected" Value="True">
                <Setter TargetName="PanelBorder" Property="Border.Background" Value="Black"/>
                <Setter TargetName="Panel" Property="Panel.Background" Value="Black"/>
                <Setter TargetName="PanelBorder" Property="Border.BorderBrush" Value="Black"/>
