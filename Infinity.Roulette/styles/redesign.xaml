    <Style x:Key="rbRestyle" TargetType="{x:Type RadioButton}">
        <Setter Property="Control.FontFamily" Value="Century Gothic"/>
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type RadioButton}">
                    <BulletDecorator Background="#FF282828" Cursor="Hand">
                        <BulletDecorator.Bullet>
                            <Grid Height="25" Width="25">
                                <Border Name="RadioOuter" Background="Transparent"
                        BorderBrush="{StaticResource solid-color-brush-lime-green}" BorderThickness="5" CornerRadius="50"/>
                                <Border Name="RadioMarkInner" Margin="2" Background="#FF282828" CornerRadius="50" Visibility="Visible"/>
                                <Border Name="RadioMark" Margin="6" Background="{StaticResource solid-color-brush-lime-green}"
                        CornerRadius="50" Visibility="Hidden"/>
                            </Grid>
                        </BulletDecorator.Bullet>
                        <TextBlock Margin="5,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center"
                       Foreground="{StaticResource solid-color-brush-lime-green}" FontFamily="Century Gothic"
                       FontSize="18" FontWeight="Bold">
              <ContentPresenter/>
                        </TextBlock>
                    </BulletDecorator>
                    <ControlTemplate.Triggers>
                        <Trigger Property="ToggleButton.IsChecked" Value="true">
                            <Setter TargetName="RadioMark" Property="UIElement.Visibility" Value="Visible"/>
                            <Setter TargetName="RadioOuter" Value="{StaticResource solid-color-brush-lime-green}"
                      Property="Border.BorderBrush"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
